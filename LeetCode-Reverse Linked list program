# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def reverseList(self, head: Optional[ListNode]) -> Optional[ListNode]:
        if head==None:
            return head
        res=head
        ptr=None
        while res is not None:
            temp=res.next
            res.next=ptr
            ptr=res
            res=temp
        return ptr

Input: head = [1,2,3,4,5]
Output: [5,4,3,2,1]
