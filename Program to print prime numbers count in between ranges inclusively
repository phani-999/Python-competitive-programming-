import math
n=2000001
siv=[1]*n
siv[0]=0
siv[1]=0
a=int(math.sqrt(n))
for i in range(2,a+1):
    if siv[i]==1:
        for j in range(i*i,n,i):
            siv[j]=0
t = int(input())
for i in range(t):
    start, end = map(int, input().split())
    count = 0
    for i in range(start, end+1):
        if siv[i] == 1:
            count += 1
    print(count)
    
input: 
4
1 8
2 12
15 25
2 5

output:
4
5
3
3
